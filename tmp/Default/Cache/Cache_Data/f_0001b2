	function slider_create(slider_id,kol,move,speed){
	var innp = document.createElement("input");
		innp.type = 'hidden';
		innp.className = 'slider_counter';
		innp.value = 0;
		$(slider_id).prepend(innp);
	
	
	$(slider_id).find('.arrow2').live('click', function(){
				 								
			var nn = innp.value-0;
			if (nn<$(slider_id).find('.item').length-kol){
			$(slider_id).find('.overflow ul').animate({'left':'-='+move},speed);
			nn++;
			}
			else {
				
			$(slider_id).find('.overflow ul').animate({'left':'0px'},speed);
			nn=0;
			}
			innp.value = nn;
														})
	$(slider_id).find('.arrow1').live('click', function(){
			var nn = innp.value-0;
			if (nn>0){
			$(slider_id).find('.overflow ul').animate({'left':'+='+move},speed);
			nn--;
			}
			else {
			nn=$(slider_id).find('.item').length-kol;
			move2 = '-='+nn*move;
			$(slider_id).find('.overflow ul').animate({'left':move2},speed);
			
			}
			innp.value = nn;
														})
}

function get_total_price(){
		var total_price = 0;
		var value1 = $('#amount').val()-0;
		var value2 = $('#quontity_inp').val()-0;

        var price_3ddd_pro_rub=$('#config_price_3ddd_pro_rub').attr('value')-0;
        var price_3ddd_free_rub=$('#config_price_3ddd_free_rub').attr('value')-0;
	    var currency_rate_eur = $('#config_currency_rate_eur').attr('value')-0;
	    var is_ps_eur = $('#is_ps_eur').attr('value')-0;
		
		
			if (value1==0){
			if (value2!=2 && value2!=0){
							$('#for_sms_payment, #sms_payment').css({'display':'none'})
						}
						else {
							$('#for_sms_payment').css({'display':'block'})
						}
			}
			else {
				$('#for_sms_payment, #sms_payment').css({'display':'none'})
			}
			
		
		total_price = value1*price_3ddd_free_rub + value2*price_3ddd_pro_rub;

		var total_price_eur = Math.round(total_price/currency_rate_eur*100)/100;
	
		$('#models_buy_form input[name="total_price_inp"]').val(total_price);
		$('.purchase_block .amount .amount_number').html(':<span>'+total_price+'<span class="rub">P</span><span class="price_eur '+(is_ps_eur? 'active':'')+'" >('+total_price_eur+' &euro;)</span></span>')
	
	if (value1>0 || value2>1){
		$('.purchase_block .amount .sub_button_green').removeAttr("disabled").removeClass('button_grey unactive');
	}
	else {

		$('.purchase_block .amount .sub_button_green').addClass('button_grey unactive').attr("disabled","disabled"); 
		 
	}
	if (($('#amount').val()-0)>0 && ($('#quontity_inp').val()-0)<=0){
		$('#amount_box').attr('class','box grey long').css({'display':'block'}).html('<div class="big">30 FREE</div><div class="small">    '+value1+' .<br>   00-00  </div>');
		$('#quontity_box').css({'display':'none'});
		$('.purchase_block .your_choise .plus').css({'display':'none'});
	}
	else if (($('#amount').val()-0)<=0 && ($('#quontity_inp').val()-0)>0){
		$('#quontity_box').attr('class','box long').css({'display':'block'}).html('<div class="big">'+value2+' PRO</div><div class="small"> </div>');
		$('#amount_box').css({'display':'none'});
		$('.purchase_block .your_choise .plus').css({'display':'none'});
	}
	else if (($('#amount').val()-0)>0 && ($('#quontity_inp').val()-0)>0){
		$('#quontity_box').attr('class','box').css({'display':'block'}).html('<div class="big">'+value2+' PRO</div><div class="small"> </div>');
		$('#amount_box').attr('class','box grey').css({'display':'block'}).html('<div class="big">30 FREE</div><div class="small">    '+value1+' .   00-00  </div>');
		$('.purchase_block .your_choise .plus').css({'display':'block'});
	}
	else {
		$('.purchase_block .your_choise .plus').css({'display':'block'});
		$('#quontity_box').attr('class','box').css({'display':'block'}).html('<div class="big null">0 PRO</div>');
		$('#amount_box').attr('class','box grey').css({'display':'block'}).html('<div class="big null">0 FREE</div>');
	}
	}

