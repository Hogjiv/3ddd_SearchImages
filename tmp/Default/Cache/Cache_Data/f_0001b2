        var link = $(this);
        var width_link = link.width();
        if (popup.is(':hidden')) {
            popup.show();
            var width = popup.outerWidth();
            var pos = -width/2 + width_link/2;
            popup.before();
            popup.css('left', pos);
        } else {
            popup.hide()
        }
    });
    $('a[data-type="question-link"]').mouseout(function () {
        $(this).parent().find('[data-toggle="question-toggle"]').hide()
