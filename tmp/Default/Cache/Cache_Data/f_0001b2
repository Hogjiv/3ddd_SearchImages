    $('.user_finance_moderation_edit').live('click',function(){

        var methods={
            mc_webmoney: 'Webmoney',
            mc_paypal: 'Paypal',
            mob_balance: 'Mobile'
        };

        var method_select='<select class="select_withdraw_method">';
        var method_select_option='';
        var selected='';

        var method=$(this).closest('tr').find('.withdraw_method').text();
        var wallet=$(this).closest('tr').find('.withdraw_wallet').text();

        $.each(methods, function(key, value) {
            if(key==method){
                selected='selected="selected"';
            }else{
                selected='';
            }
            method_select_option='<option value="'+key+'" '+selected+'>'+value+'</option>';
            method_select=method_select+method_select_option;
        });
        method_select=method_select+'</select>'

        $(this).closest('tr').find('.withdraw_method').html(method_select);

        var wallet_field_html='<input type="text" value="'+wallet+'">';

        if(method=='mob_balance'){
            wallet_field_html=wallet_field_html+$('#mob_operatos_list').html();
        }

        $(this).closest('tr').find('.withdraw_wallet').html(wallet_field_html);

        var actions = $(this).closest('.user_finance_moderate_actions').html('');

        actions.html('<a href="'+actions.attr('save_link')+'" class="user_finance_moderation_save"></a>, <a href="" class="user_finance_moderation_cancel"></a>');

        return false;
    });

    $('.select_withdraw_method').live('change',function(){

        if($(this).val()=='mob_balance'){
            $(this).closest('tr').find('.withdraw_wallet').append($('#mob_operatos_list').html());
        }else{
            $(this).closest('tr').find('.withdraw_wallet').find('.select_mobile_operators').remove();
        }

    });

    $('.user_finance_moderation_save').live('click',function(){

        var method=$(this).closest('tr').find('.withdraw_method select').val();
        var wallet=$(this).closest('tr').find('.withdraw_wallet input').val();
        if(method=='mob_balance'){
            var operator=$(this).closest('tr').find('.withdraw_wallet select').val();
            var extra_array={'mobile_id_operator': operator};
            var extra= JSON.stringify(extra_array);
        }else{
            var extra='';
        }

        var actions=$(this).closest('.user_finance_moderate_actions');

        var context=$(this);

        $.post($(this).attr('href'),
            {
                method: method,
                wallet: wallet,
                extra: extra
            },
            function(data){
                if(data=='ok'){

                    context.closest('tr').find('.withdraw_method').html(method);

                    context.closest('tr').find('.withdraw_wallet').html(wallet);

                    actions.html('<a href="" class="user_finance_moderation_edit"></a>');

                }else{
                    alert(' ');
                }
            }
        );

        return false;

    });

    $('.user_finance_moderation_cancel').live('click',function(){

        var method=$(this).closest('tr').find('.withdraw_method select').val();
        var wallet=$(this).closest('tr').find('.withdraw_wallet input').val();

        $(this).closest('tr').find('.withdraw_method').html(method);

        $(this).closest('tr').find('.withdraw_wallet').html(wallet);

        $(this).closest('.user_finance_moderate_actions').html('<a href="" class="user_finance_moderation_edit"></a>');

        return false;

    });

    //Untie users
    $('.link_untie').click(function(){

        var context=$(this);

        $.get($(this).attr('href'),function(data){
            if(data!='error'){
                context.closest('tr').find('.evercookie_hash').html(data);
                context.remove();
            }else{
                alert('Error');
            }
        });
        return false;
    });

    //Withdraw form  link click
    $('#withdraw_method_save').click(function(){
        if ($('#method_conclusion_select').val()=='#mc_webmoney'){

            function erorF(input){
                input.addClass('error');
                return false
            }
            function removeErorF(input){
                input.removeClass('error');

            }

            if($('#pay_choice_form')[0].checkValidity() === false){
                $('#mc_webmoney input').each(function () {
                    if($(this).prop('required') === true && $(this).val() === ''){
                        erorF($(this))
                    }else {
                        removeErorF($(this))
                    }
                });
                $('#mc_webmoney .custom-select').each(function () {
                    let select = $(this).find('select');
                    if(select.prop('required') === true && select.val() === ''){
                        erorF($(this))
                    }else {
                        removeErorF($(this))
                    }
                });
                return false;
            }else {
                var input = $('#mc_webmoney input[name="account[mc_webmoney]"]');
                var value = input.val();
                var temp = value.split('');
                var dfg = value.substring(0, 3);
                if ( temp[0]=='U' || temp[0]=='Z' || temp[0]=='R'){

                    value = value.replace(temp[0], '');
                    var rep = /[a-zA-Z--]/;

                    if (rep.test(value)) {
                        return erorF(input)
                    }
                    if (value.length!=12) {
                        return erorF(input)
                    }

                }
                else {
                    return erorF(input)
                }
            }

        }
        else if ($('#method_conclusion_select').val()=='#mc_paypal'){
            var regex = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
            if(!regex.test($('#mc_paypal input[type="text"]').val())){
                $('#mc_paypal input[type="text"]').addClass('error');
                return false
            }
        }
        else if  ($('#method_conclusion_select').val()=='#mob_balance'){
            var value = $('#mob_balance input[type="text"]').val();
            var temp = value.split('');
            var rep = /[a-zA-Z--]/;
            /*value = value.replace(temp[0], '');
             if (temp[0]!='+'  || rep.test(value)){
             $('#mob_balance input[type="text"]').addClass('error');
             return false
             }*/
            //var regexp=/\+?[7|38]\d{10}/;
            var regexp=/^\+?((70|71|72|74|75|76|78|79)\d{9}|38\d{10})$/;
            if(!regexp.test(value)){
                $('#mob_balance input[type="text"]').addClass('error');
                return false;
            }
            if(!$('#operator_select').val()){
                $('#operator_select').addClass('error');
                return false;
            }

            value = value.replace('+', '');
            $('#mob_balance input[type="text"]').val(value);
        }
        else {

        }

        var $method=$('#method_conclusion_select').val();
        if($method==''){
            //alert('{% trans %}profile.withdraw.choode.ps{% endtrans %}');
            $('#method_conclusion_select').addClass('error');
            return false;
        }
        var $wallet=$($method).find('.text_button').val();
        if($wallet==''){
            //alert('{% trans %}profile.withdraw.fill.wallet{% endtrans %}');
            $($method).find('.text_button').addClass('error');
            return false;
        }
        //var $extra=$('#operator_select').val();
        var $extra = {};
        if($('[name*="extra"]').length){
            $('[name*="extra"]').each(function () {
                let key = $(this).attr('name');
                $extra[key] = $(this).val()
            })
        }

        $method=$method.replace('#','');
        var $url=$(this).attr('href');
        $url=$url+'?method='+$method+'&wallet='+$wallet+'&'+ $.param($extra);
        $(this).attr('href',$url);
        return true;

    })

    function strpos( haystack, needle, offset){	// Find position of first occurrence of a string
        //
        // +   original by: Kevin van Zonneveld (http://kevin.vanzonneveld.net)

        var i = haystack.indexOf( needle, offset ); // returns -1
        return i >= 0 ? i : false;
    }


    //ajax  moderate tags list
    $('.tags_moderation_toggle').change(function(){

        var context=$(this);

        $.get($(this).attr('href'),function(data){
            if(data=='ok_on'){
                context.prop('checked', true);
            }else{
                if(data=='ok_off'){
                    context.prop('checked', false);
                }else{
                    alert(' ');
                }
            }
        });

        return false;
    });

    $('.tags_moderation_edit').live('click',function(){

        var title=$(this).closest('tr').find('.tag_title').text();

        var title_field_html='<input type="text" value="'+title+'">';

        $(this).closest('tr').find('.tag_title').html(title_field_html);

        var actions = $(this).closest('.tags_moderate_actions').html('');

        actions.html('<a href="'+actions.attr('save_link')+'" class="tags_moderation_save"></a>, <a href="" class="tags_moderation_cancel"></a>');

        return false;
    });

    $('.tags_moderation_save').live('click',function(){

        var tag_title=$(this).closest('tr').find('.tag_title input').val();

        var actions=$(this).closest('.tags_moderate_actions');

        var context=$(this);

        $.post($(this).attr('href'),
            {
                title: tag_title
            },
            function(data){
                if(data=='ok'){

                    context.closest('tr').find('.tag_title').html(tag_title);

                    actions.html('<a href="" class="tags_moderation_edit"></a>');

                }else{

                    if(data=='ok_reload'){
                        window.location.reload();
                    }else{
                        alert(' ');
                    }
                }
            }
        );

        return false;

    });

    $('.tags_moderation_cancel').live('click',function(){

        var tag_title=$(this).closest('tr').find('.tag_title input').val();

        $(this).closest('tr').find('.tag_title').html(tag_title);

        $(this).closest('.tags_moderate_actions').html('<a href="" class="tags_moderation_edit"></a>');

        return false;

    });

