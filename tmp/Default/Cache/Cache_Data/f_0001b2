$(document).ready(function(){
	

	if ($('.alarm').length){
		$('#header').css({'margin-bottom':'40px'})
	$('.alarm .close').click(function(){
		$(this).parents('.alarm').css({'display':'none'})
		$('#header').css({'margin-bottom':'0px'})
		})
	}
if ($('.time_date_left').length>0){
		
		
		function end_word(word,end1,end2,end3){
				var years = Math.round(word)+'';
				var z_lenth = years.length;
				var endx = years[z_lenth-1];
				if ((years-0)>10 && (years-0)<20){
					end = end1;
				}
				else if (endx==='1'){
					end = end2;
				}
				else if (endx>1 && endx<5){
					end = end3;
				}
				else {
					end = end1;
				}
				
				var otvet = years + ' '+end;	
				return otvet;
		}

function times_lefts(){
	var d = new Date();
		var cur_date = d.getTime();

			$('.time_date_left').each(function(){
					var vzdate = $(this).attr('val');
					var otnim = cur_date-vzdate;
					var seconds  = otnim/1000;
					var minuts  = otnim/1000/60;
					var hours  = otnim/1000/60/60;
					var days  = otnim/1000/60/60/24;
					var weeks  = otnim/1000/60/60/24/7;
					var mothes  = otnim/1000/60/60/24/31;
					var years  = otnim/1000/60/60/24/366;
				
				if (years>1){
			 var otver2 = end_word(years,'','','');
				}
				else if (mothes>1){
				 var otver2 = end_word(mothes,'','','');				 
				}
				else if (weeks>1){
				 var otver2 = end_word(weeks,'','','');
				}
				else if (days>1){
				 var otver2 = end_word(days,'','','');
				}
				else if (hours>1){
				 var otver2 = end_word(hours,'','','');
				}
				else if (minuts>1){
				 var otver2 = end_word(minuts,'','','');
				}
				else {
				 var otver2 = end_word(seconds,'','','');
				}
				
				var otver  = otver2+ ' ';
					
					$(this).text(otver)
										   })
}
		
		setInterval(function(){
							
			times_lefts()							
							  },30000);
			times_lefts()
}

	   	if ($('#top_link').length){
				
				function knowScroll(){
				
	var gCT = self.pageYOffset || (document.documentElement && document.documentElement.scrollTop) || (document.body && document.body.scrollTop);
		
			if (gCT>250){
					$('#top_link').css({'display':'block'}).attr('class','');
					
						}
						else {
					$('#top_link').css({'display':'none'});			
						}
}		

		$('#top_link').bind('click', function(){
					if (this.className==''){	
	bzTop = self.pageYOffset || (document.documentElement && document.documentElement.scrollTop) || (document.body && document.body.scrollTop);
	$('html, body').animate({scrollTop: "0px"},100, function(){ setTimeout(function(){
				$('#top_link').css({'display':'block'}).attr('val',bzTop).attr('class','top_cr'); 
				},10); });
	
					}
					else {
				var vv = $('#top_link').attr('val');		
				$('html, body').animate({scrollTop: bzTop+"px"},100);
				$('#top_link').attr('val','0');	
				this.className='';
					}
	return false
	})	
		
			knowScroll();
			$(window).scroll(function(){
				knowScroll();				  
									  })
				}

