            rows = $('[data-events="toggle-text-with-less"]').data('show'),
            height = $('[data-events="toggle-text-with-less"]').height(),
            lineHeight = parseFloat($('[data-events="toggle-text-with-less"]').data('height')),
            heightOrig = lineHeight*rows,
            showLink = $('[data-events="toggle-text-with-less"] .show-all')
            hideLink = $('[data-events="toggle-text-with-less"] .hide-all')

        if(height>heightOrig){
            text.css('height', heightOrig+'px')
            showLink.show().css('display','inline-flex')
        }

        $('[data-events="toggle-text-with-less"] a.show-all').click(function (e) {
            e.preventDefault();
            let text = $(this).closest('[data-events="toggle-text-with-less"]').find('p')
            text.animate({
                height: height
            },0)
            showLink.hide();
            hideLink.show().css('display','inline-flex');
        })
        $('[data-events="toggle-text-with-less"] a.hide-all').click(function (e) {
            e.preventDefault();
            let text = $(this).closest('[data-events="toggle-text-with-less"]').find('p')
            text.animate({
                height: heightOrig
            },0)
            showLink.show().css('display','inline-flex');
            hideLink.hide();
        })

    }
    if ($('#pirate').length) {
        let popup = $('#pirate')
    }
    if ($('[data-events="user_pirate"]').length) {
        let popup = $('.moderate-pirate-popup');
        $('[data-events="user_pirate"]').click(function (e){
            if ($(popup).is(":hidden")) {
                popup.show();
            } else {
                popup.hide();
            }
        });
        $(popup.find('.sub_button_green')).click(function (e){
            popup.hide();
        })
