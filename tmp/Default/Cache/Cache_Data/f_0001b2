            var label = $(this).closest('.birka_position');
            label.removeClass('draggable').removeClass('ui-draggable').addClass('toggle-link2 green');
            $(this).removeClass('birka_big_passive').addClass('birka_big_active');

            var elem = $(this).parent().parent();
            var width = elem.width();
            var left = $(this).parent().position().left;
            if (left > width / 2) {
                var popup = $(this).parent().find('.toggle-window2').removeClass('ramka-link_right').addClass('ramka-link_left');
                if (popup.is(':hidden')) {
                    popup.show()
                } else {
                    popup.hide()
                }

            } else {
                var popup = $(this).parent().find('.toggle-window2').removeClass('ramka-link_left').addClass('ramka-link_right');
                if (popup.is(':hidden')) {
                    popup.show()
                } else {
                    popup.hide()
                }
            }
            e.preventDefault();
        });
    }






