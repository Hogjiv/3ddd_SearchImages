// Show contacts for item pages
//===================================
	$('#author-contacts').on('show.bs.collapse', function () {
		$(".show-contacts").css('display', 'none');
	});


	//  new color picker

	if($('.color-picker-link').length>0){
        $('.color-picker-link').click(function () {
			$(this).next('.colorpickerplus').show();

        })
	}

	var user_feeds = document.getElementById('user_feeds');
	var bell_link = document.getElementById('top_bell_link');
	var page = $('html, body');
	if (user_feeds) {
		if (bell_link) {
			bell_link.addEventListener('click', function (e) {
				e.preventDefault();
				page.animate({
					scrollTop: $(user_feeds).offset().top
				}, 400);
			})
		}
		var userFeedsHash = location.hash;
		location.hash = '';
		if (userFeedsHash === '#user_feeds') {
			page.animate({
				scrollTop: $(user_feeds).offset().top
			}, 400);
		}
	}

	$('#reduct_popup form').on('submit', function(e){
		if ($('#reduct_popup form #user-id-rubric').length>0) {
			e.preventDefault();
			let form = $(this);
			let actionUrl = form.attr('action');
			let count = $('.autosize').val().length;
			let title = $('#name-rubric').val();
			let dataObj = {
				'title': title,
				'user_id': $('#user-id-rubric').val()
			}
			$('body').css('cursor', 'wait')
			$.ajax({
				url: '/api/bookmarks/validate',
				type: "POST",
				dataType: "json",
				contentType: "application/json",
				data: JSON.stringify(dataObj),
				success: function (data) {
					let maxCount = 1000;
					if (count > maxCount) {
						$('#reduct_popup .red-error').html('     ' + maxCount + ' ');
					} else {
						$.ajax({
							type: "POST",
							url: actionUrl,
							data: form.serialize(), // serializes the form's elements.
							success: function (data) {
								location.reload()
								$('body').css('cursor', 'default')
							}
						});
					}
				},
				error: function (data) {
					try {
						result = JSON.parse(data.responseText);
						if ('message' in result) {
							let message = result['message'];
							if (message === 'collection_already_exists') {
								$('#reduct_popup .red-error-name').html('       ');
							} else {
								$('#reduct_popup .red-error-name').html(' ');
							}
						}
					} catch (e) {
						$('#reduct_popup .red-error-name').html(' ');
					}
					$('body').css('cursor', 'default')
				}
			});
		} else {
			count = $('.autosize').val().length;
			let maxCount = 1000;
			if (count>maxCount) {
				$('#reduct_popup .red-error').html('     ' + maxCount + ' ');
				return false;
			}
			return true;
		}
	})

});

//    3DDD  SKY

$(document).ready(function () {

    // Birka

    if($('.dropdown-menu a.toggle-link').length>0) {
        $(document).on('click', '.dropdown-menu a.toggle-link', function (e) {

            e.stopImmediatePropagation();
            var link = $(this);
            var elem = $(this).parent().find('.toggle-window');
            if (elem.is(':hidden')) {
                var lis = $('.birka_menu li');
                var li = $(this).closest('.birka_menu li');
                if(li.is(':last-child') && lis.length>1){
                    elem.css('top', '-120px')
                }
                elem.show();
            } else {
                $('.dropdown-menu a.toggle-link .btn_close').click(function () {
                    elem.hide();
                })
            }
            return false;
        });
    }

    if($('.delete-dropdown-menu .btn_close').length>0) {
        $('.delete-dropdown-menu .btn_close').click(function () {
            $(this).parent().parent().hide();
        });
    }



    //    
    if($('.block-birka-menu a').length>0) {
        $('.block-birka-menu > a').click(function () {
            // var elem = $('.toggle-link2').parent();
            var elem = $(this).parent().find(".ramka-link_down");

            if (elem.is(':hidden')) {
                elem.show();
            } else {
                elem.hide();
            }

            return false;

        });
    }
    //      
    $(document).mouseup(function (e) {
        if (!$(e.target).closest(".ramka-link_down").length) {
            $('.ramka-link_down').hide();
        }
        if (!$(".toggle-window2").is(e.target) && $(".toggle-window2").has(e.target).length === 0 && $('.toggle-link2').has(e.target).length === 0) {
            $('.toggle-window2').hide();
        }
    });


    //     
    if($('#galleries-block').length>0) {
        $('#galleries-block').on('click', '[data-events="btn-close"]', function () {
            // $('#btn-close').click(function () {
            // var elem = $('.toggle-link2').parent();
            var elem = $(this).closest('.toggle-window2');

            if (elem.is(':hidden')) {
                elem.show();
            } else {
                elem.hide();
            }

            return false;

        });
    }

    //  

    if($('#add_new_birka').length>0){
        $('#add_new_birka').click(function (e) {
            e.preventDefault();
            var birka_menu = $('.birka_img_block .birka');
            birka_menu.removeClass('birka_passive').addClass('birka_active');
            $('.birka_position.toggle-link2').each(function () {
                $(this).css('display', 'block')
            });
            var gal = $('#galleries-block');
            var pic = gal.find('.item').first();
            var heightPos = 150+pic.position().top;
            var widthPos = parseInt($('#galleries-block').width())/2;
            var canvas = ("<div class='birka_position draggable' style='left: "+widthPos+"px; top:" + heightPos +"px '>" +
                "<a class='birka_big_passive'>+</a>" +
                "<div class='toggle-window2'><h5>   </h5><form action=''><input name='add_link' type='text'><div class='btn_flex m-b-15 birka-upload-model'>\n" +
                "                                                  <div class='loader' >\n" +
                "                                                      <img src=\"/assets/skypages/images/loader.svg\">\n" +
                "                                                  </div>\n" +
                "                                                  <div class='upload-img-block' >\n" +
                " 													<div class='' >\n" +
                "                                                      <img src=\"/assets/skypages/images/loader.svg\">\n" +
                "                                                  </div>\n" +
                "                                                  <div>\n" +
                "                                                      <b class=\"preview_text\" ><span class=''>PRO</span>\n" +
                "                                                          <a href=\"#\" class=\"name_preview\"></a></b>\n" +
                "                                                  </div>\n" +
                "                                                  </div>\n" +
                "                                              </div><div><button class='btn-sm btn-green' type='submit' data-events='save-label'></button><a class='btn-sm btn-dark-grey' data-events='btn-close'></a></div></form></div>"+
                "</div>");

            var overlapWindow=25;
            var minLeft=widthPos-overlapWindow;
            var minTop=heightPos-overlapWindow;
            var maxLeft=widthPos+overlapWindow;
            var maxTop=heightPos+overlapWindow;
            $('li.item:first-child .birka_position').each(function (idx, elem) {
                var elem_left=parseInt($(elem).css('left'));
                var elem_top=parseInt($(elem).css('top'));
                console.log("left:"+elem_left+" top:"+elem_top);
                console.log("height:"+heightPos+" wigth:"+widthPos);
                if(elem_left>minLeft && elem_left<maxLeft && elem_top>minTop && elem_top<maxTop){
                    var new_canvas=markLabelOverlap($(canvas));
                    canvas=$('<div />').append(new_canvas.clone()).html();
                }
            });
            gal.append(canvas);
            var dropEl = $('[data-target="droppable"]')["0"];



            $(".draggable").draggable({
                cursor: 'move',
                delay: 200,
                containment: 'parent',
                create: function (event) {
                    $('[data-target="droppable"]').each(function (index) {
                        if(index === 0){
                            $(this).droppable({
                                create:function(){
                                    var id = $(this).data('id');
                                    var pos = $(this).offset();
                                    $('[data-events="save-label"]').attr('data-id', id).attr('data-x', pos.left).attr('data-y', pos.top);

                                }
                            });
                        }

                    });
                    $( ".birka_position.toggle-link2" ).droppable({
                        over: function( event, ui ) {
                            // ui.helper.css('filter', 'drop-shadow(0 0 16px red)');
                            markLabelOverlap(ui.helper);
                        },
                        out: function (event, ui) {
                            unmarkLabelOverlap(ui.helper);
                        }
                    });

                    $( ".birka_position.draggable.ui-draggable" ).droppable({
                        over: function( event, ui ) {
                            // ui.helper.css('filter', 'drop-shadow(0 0 16px red)');
                            markLabelOverlap(ui.helper);
                        },
                        out: function (event, ui) {
                            unmarkLabelOverlap(ui.helper);
                        }
                    });

                    $('[data-target="droppable"]').droppable({
                        drop:function(){
                            var id = $(this).data('id');
                            var pos = $(this).offset();
                            $('[data-events="save-label"]').attr('data-id', id).attr('data-x', pos.left).attr('data-y', pos.top);

                        }
                    });

                },
                start: function(event) {

                },
                drag: function( event, ui ) {

                },
                stop: function(event) {
                    $(this).position();
                }
            });


        });

        function markLabelOverlap(elem){
            elem.css('filter', 'drop-shadow(0 0 5px red)');
            elem.addClass('overlap');
            elem.addClass('no-birka');
            elem.find('[data-events="save-label"]').prop('disabled', true);
            console.log('over in');
            return elem;
        }

        function unmarkLabelOverlap(elem){
            elem.css('filter', '');
            elem.removeClass('overlap');
            elem.removeClass('no-birka');
            elem.find('[data-events="save-label"]').prop('disabled', false);
            console.log('over out');
        }

    }

    $('#galleries-block').on('click', '[data-events="btn-close"]', function () {
        var b1 = $(this).closest('.birka_position.draggable.ui-draggable');
        if(b1.length == 1){
            b1.remove();
        }else {
            var b2 = $(this).closest('.birka_position.toggle-link2.green');
            if(b2.length == 1) {
                b2.remove();
            }
        }

    });


    $( ".birka_position.toggle-link2" ).droppable({

    });


