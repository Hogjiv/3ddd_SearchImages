            source: function (request, response) {
                jQuery.get("/app_dev.php/search/hint_new/", {
                    query: request.term
                }, function (data) {
                    response(data);
                });
            },
            minLength: 3,
            open: function( event, ui ) {
                $('.autocomplete_search-menu').css('border-radius', '3px 3px 0 0')
            },
            select: function (event, ui) {
                $(this).closest('form').submit()
            },
            close: function (event, ui) {
                $('.autocomplete_search-menu').css('border-radius', '3px 3px 3px 3px')
            }
        }).autocomplete( "widget" ).addClass( "new-menu-search-block" );
    } );

