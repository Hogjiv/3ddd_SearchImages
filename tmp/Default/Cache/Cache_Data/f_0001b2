    //     input-
    $(document).on('change', '[data-events="upload_model"] input', function () {

        var fileName = $(this)[0].files[0].name,
            fileSize = $(this)[0].files[0].size,
            fileType = $(this)[0].files[0].type,
            dataType = $(this).data('type'),
            time = fileSize/10000,
            maxSize = $(this).data('size');

            var checkType = checkFileType(dataType, $(this)[0].files[0]);

            if(checkType.status === 'error'){
                console.log(checkType.text);
                $(this).parent().before('<label class="error">'+checkType.text+'</label>');
                return false
            }

            if(fileSize > maxSize){
                console.log('     '+maxSize/Math.pow(1024,2)+'Mb');
                $(this).parent().before('<label class="error">'+'     '+maxSize/Math.pow(1024,2)+'Mb'+'</label>');
                return false
            }
            // uploadFileProgress($(this)[0].files[0])
        var extblock = $(this).closest('.uploads-block-item');
        extblock.find('[data-events="upload_model"]').css('display', 'none');
        var block =  extblock.find('[data-target="upload_model"]');

        var fSExt = new Array('Bytes', 'KB', 'MB', 'GB'),
            i=0;while(fileSize>900){fileSize/=1024;i++;}
        var exactSize = (Math.round(fileSize*100)/100)+' '+fSExt[i];

        block.css('display','block');
        progress(block, time/100);
        setTimeout(function () {
            block.css('display','none');
            var finishBlock = extblock.find('[data-target="upload_model_finish"]');
            finishBlock.find('.p-size').text(exactSize);
            finishBlock.find('.render-a').text(fileName);
            finishBlock.css('display','block')
        }, time);
    });



