    $( ".subscriptions-list-display-btn" ).click(function(e) {
        e.preventDefault();
        let list = $(this).closest('.subscriptions-block').find(".subscriptions-list");
        let listHeight = list.height();
        let btn = $(this);

        if (listHeight === 116) {
            list.css('height', 'auto')
            btn.text(btn.data('trans-collapse'))
            btn.removeClass('collapsed')
        } else {
            list.css('height', '116px');
            btn.text(btn.data('trans-expand'))
            btn.addClass('collapsed')
        }
    });

    autosize($('.autosize'));

    if ($('[data-events="toggle-text-with-less"]').length) {
