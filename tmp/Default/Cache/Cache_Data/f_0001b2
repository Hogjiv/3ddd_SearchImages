            var maxFileSize = $(this).data('max-size'),
                allowedExt = $(this).data('allowed-ext'),
                fileSize = e.target.files[0].size,
                fileName = e.target.files[0].name
            ;
            $(this).closest('#change_photo_popup').find('.error-file').remove()
            if (fileSize > maxFileSize || (checkExt(allowedExt, fileName) < 0)) {

                $(this).closest('.input_file').after('<span class="error-file" style="color: red">    </span>')
            }
        })
    }

});

function checkExt(allowedExt, fileName) {
    let ext = fileName.split('.');

    return $.inArray(ext[ext.length - 1].toLowerCase(), allowedExt.split(','));
}

/**
 *
 * @param obj
 * @param delay
 */
function showFirstSubmenu(obj, delay) {
    let $fs = obj.find('>ul'),
        height = $fs.outerHeight(),
        width = $fs.width(),
        timeoutId = setTimeout(function(){
            $('.first-submenu>li').css('background-color', '#fff').find('>a').css('color', '#333');
            $fs.find('>li.first-submenu-all-models').next().css('background-color', '#F0F5FA');
            $fs.find('>li.first-submenu-all-models').next().find('>a').css('color', '#4182aa');
            $fs.show();
            // $('.first-submenu-background').show();

            $fs.find('>li.first-submenu-all-models').next().find('.second-submenu').css('height', height).css('width', 960 - width).css('left', width).show();
        }, delay);
    obj.data('timeoutId', timeoutId);
}

/**
 *
 * @param obj
 * @param delay
 */
function hideFirstSubmenu(obj, delay) {
    clearTimeout(obj.data('timeoutId'));
    let $fs = obj.find('>ul');
    setTimeout(function () {
        $fs.hide();
        $fs.find('>li:first-child .second-submenu').hide();
        $('.second-submenu').hide();
    }, delay);
}
function statEvent(type, val){

    let val_str = '';

    if($.isPlainObject(val)){
        let keys = Object.keys(val);
        for(i=0; i<keys.length; i++){
            let k=keys[i];
            let v=val[k];
            val_str = val_str + '&val_' + k + '=' + v;
        }
    }else{
        val_str = '&val='+val;
    }

    $.get('/api/stat/event?_sa=1&type='+type+val_str);
}
$(document).ready(function () {
  let dropdown = $('.ang-avatar-dropdown-block');
  let avatar = $('.avatar-name');
  let state = dropdown.hasClass('collapsed');

  $('#avatar-frame').on({
    mouseenter: function () {
      setTimeout(() => {
        dropdown.addClass('collapsed')
        avatar.addClass('collapsed')
        dropdown.show();
      }, 125);
    },
    mouseleave: function () {
      setTimeout(() => {
        dropdown.removeClass('collapsed')
        avatar.removeClass('collapsed')
        dropdown.hide()
      }, 125);
    }
  })

  $('#query_search').focus(function (){
    let input = $('#query_search');
    // input.addClass('search-not-empty');
    let height = $( document ).height();
    $('#wrap-background').addClass('search-background').css('height', height + 'px').css('z-index', 40);
    if (hintsCount > 0) {
      $('#query_search').addClass('hints-found');
      $('#typeahead').show();
    }
  })

  $('#wrap-background').click(function (){
    $(this).removeClass('search-background').css('z-index', -100)
  });

});

$(document).mouseup(function (e) {
  if (!$(e.target).closest('#typeahead').length && !$(e.target).closest('#query_search').length) {
    $('#typeahead').hide();
    $('#query_search').removeClass('hints-found')
  }

  $('#search_glass').click(function (){
    const query = $('#query_search').val();
    if (query.length >= 3) {
      goToSearch(query);
    }
  })
});



let hints;
let hintsCount;
let activeHint = -1;

function onTypeahead(e){
  let input = $('#query_search')
  const query = input.val();

  if (e.key === 'ArrowDown') {
    if (activeHint < hintsCount - 1) {
      activeHint++;
      menuItemArrow(activeHint)
    }
    return;
  }
  if (e.key === 'ArrowUp') {
    if (activeHint > 0) {
      activeHint--;
      menuItemArrow(activeHint)
    }
    return;
  }
  if (e.key === 'Enter') {
    const query = $('#query_search').val();
    if (query.length >= 3) {
      goToSearch(query);
    }
    return;
  }
  if (query.length > 0) {
    $('#cleaner').show();
    $('#cleaner_vertical_line').show();
    input.addClass('search-not-empty');
  } else {
    $('#cleaner').hide();
    $('#cleaner_vertical_line').hide();
    input.removeClass('search-not-empty');
  }

  if (query.length >= 3) {
    setTimeout(function () {
      jQuery.get("/search/hint_new/", {
        query: query
      }, function (data) {
        generateHints(data)

      });
    }, 300);
  }
}

function generateHints(data) {
  hints = data;
  let typeahead = $('#typeahead');
  hintsCount = data.length;
  clearItemList()
  if (hintsCount > 0) {
    $.each(data, function (index, value){

      $('<div>', {
        class: 'item ng-star-inserted',
        text: value,
        onclick: 'hintSearch(event)',
        tabindex: '0',
        onmouseover: 'menuItemMouseOver(event)',
        onmouseleave: 'menuItemMouseLeave(event)',
        dataId: index,
      }).appendTo(typeahead);
    })
    typeahead.show();
    $('#cleaner').show()
    $('#cleaner_vertical_line').show();
    $('#query_search').addClass('hints-found')
  }
}

function cleaner() {
  clearItemList()
  $('#query_search').val('').removeClass('hints-found').removeClass('search-not-empty');
  $('#cleaner').hide()
  $('#cleaner_vertical_line').hide();
}

function clearItemList() {
  $('#typeahead').empty();
}

function menuItemMouseOver(e) {
  $('#typeahead div').each(function (index, value) {
    $(this).removeClass('active');
  })
  e.target.classList.add('active')
  activeHint = $('#typeahead .active').attr('dataId')
}
function menuItemMouseLeave(e) {
  //e.target.classList.remove('active')
}

function menuItemArrow(activeHint) {
  $('#typeahead div').each(function (index, value) {
    $(this).removeClass('active');
  })
  $('#typeahead [dataId="'+activeHint+'"]').addClass('active');
}

function hintSearch(e) {
  const searchStr = e.target.textContent;
  this.goToSearch(searchStr);
}

function goToSearch(searchStr) {
  if (searchStr.length >= 3) {
    const searchStrEncoded = encodeURIComponent(searchStr);
    window.location.href = '/search?query=' + searchStrEncoded;
  }
}



