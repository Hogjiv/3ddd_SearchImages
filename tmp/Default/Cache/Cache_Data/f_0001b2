        $(this).find('.btn-block').show();
    },function () {
        $(this).find('.btn-block').hide();
    });

    $('.bookmark_collection_line').click(function (e) {
        if($(e.target).is('input')){            
            return;
        }
        var checkbox=$(this).find('input[type=checkbox]');
        if (checkbox.hasClass('inactive')) {
            return;
        }
        checkbox.prop("checked", !checkbox.prop("checked"));
        checkbox.trigger('change');
        return false;
    });

    $('#show-contacts').click(function () {
        $('#author-contacts').toggle();
        $(this).css("display", "none");
    })
    
    //Moderate form images switch
    $('.small_preview').click(function(){
        $(this).closest('.corected').find('.big_preview').attr('src',$(this).attr('big_image'));
    });

    //    checkbox  
    if($('#project_contractPrice').length){
        if($('#project_contractPrice').is(':checked')){
            $('#project_price').prop('disabled', true );
            var select = $('#project_curPrice').selectize();
            var selectize = select[0].selectize;
            // $('#project_curPrice').prop('disabled', true );
            selectize.disable();
        }else{
            $('#project_price').prop('disabled', false);
            var select = $('#project_curPrice').selectize();
            var selectize = select[0].selectize;
            // $('#project_curPrice').prop('disabled', true );
            selectize.enable();
        }
        $('#project_contractPrice').change(function () {
            if (this.checked) {
                $('#project_price').prop('disabled', true );
                var select = $('#project_curPrice').selectize();
                var selectize = select[0].selectize;
                // $('#project_curPrice').prop('disabled', true );
                selectize.disable();

            }else {
                $('#project_price').prop('disabled', false);
                var select = $('#project_curPrice').selectize();
                var selectize = select[0].selectize;
                // $('#project_curPrice').prop('disabled', true );
                selectize.enable();
            }
        })
    }

    //   .  
    if($('.cat_filter_item').length>0){
        $('.cat_filter_item').mouseover(function () {
            if($(this).find('.collapse-menu').is(':hidden')){
                $(this).find('.icofont-rounded-down').show()
            }

        });
        $('.cat_filter_item').mouseout(function () {
            $(this).find('.icofont-rounded-down').hide()
        });

        $('.collapse-menu input.cat_checkbox:checked:first-child').each(function () {
            $(this).closest('ul.collapse-menu').slideDown(0, function () {
                $(this).parent().find('.icofont-rounded-up').toggle();
            });
        })

    }

    if($('[data-toggle="collapse-menu"]').length>0){
        $('[data-toggle="collapse-menu"]').click(function (e) {
            e.preventDefault();
            var id = $(this).attr('href');
            var menu = $(id);

            menu.slideToggle(0, function () {
                $(this).parent().find('.icofont-rounded-down, .icofont-rounded-up').toggle();
            });



        })
    }

    if($(".phone-mask").length){
        $(".phone-mask").mask("+9(999) 999-99-99");
    }

});

function show_captcha(){

    var url=$('#captcha_image_url_container').attr('captcha_url');

    $('.captcha_image').attr('src',url);

}


$(document).ready(function(){

    function strpos (haystack, needle, offset) {
        var i = (haystack+'').indexOf(needle, (offset || 0));
        return i === -1 ? false : i;
    }

