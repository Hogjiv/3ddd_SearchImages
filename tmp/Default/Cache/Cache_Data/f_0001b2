    // Sections in models by tag page
    $('#sections_list .cat_checkbox').change(function(){
        /*if($(this).is(':checked')){
            var href=$(this).attr('href');
        }else{
            var href=$(this).closest('ul').attr('href');;
        }*/

        var elems=new Array();
        var cnt=0;
        $('#sections_list .cat_checkbox').each(function(ids,elem){
            if($(elem).is(':checked')){
                elems[cnt]=$(this).attr('slug');
                cnt++;
            }
        });
        //var href=$(this).closest('ul').attr('href');
        var href=$('.cat_filter_items').attr('href');

        href=href.replace("{cats_paceholder}",elems.join(","));

        window.location=href;
    });



