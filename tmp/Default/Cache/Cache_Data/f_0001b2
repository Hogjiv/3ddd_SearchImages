    /* feedbck form */
    let feedbackTheme = $('#sky_bundle_feedbacksbundle_feedbackstype_theme');
    let feedbackForm = $('#feed_back_form select');

    if (feedbackForm.val() === ' ' || feedbackForm.val() === 'Own theme') {
        feedbackTheme.removeClass('hidden');
    }

    feedbackForm.change(function (e) {
       if ($(this).val() === ' ' || $(this).val() === 'Own theme') {
           feedbackTheme.removeClass('hidden');
           feedbackTheme.val('');
       } else {
           feedbackTheme.addClass('hidden');
           feedbackTheme.val($(this).val());
       }
    });
});


//      (    document.ready    )
function checkFileType(type, file) {
    var fileType = file.type,
        fileExtension = file.name.split('.').pop()
    ;

    if(type === 'image'){
        var mimeArr = [
            'image/jpeg',
            'image/png',
            'image/gif'
        ];

        if(!mimeArr.includes(fileType)){
            return {'status': 'error', 'text': '    .jpg, .png  .gif'}
        }

    } else if(type === 'file'){
        var mimeArr = [
            'application/x-rar-compressed', 'application/octet-stream',
            'application/zip', 'application/octet-stream', 'application/x-zip-compressed', 'multipart/x-zip'
        ];

        var extensionArr = ['rar', 'zip'];

        if(fileType!== null && fileType !== ''){
            if(!mimeArr.includes(fileType)){
                return {'status': 'error', 'text': '    .rar  .zip'}
            }
        }else {
            if(!extensionArr.includes(fileExtension)){
                return {'status': 'error', 'text': '    .rar  .zip'}
            }
        }

    }
    return {'status': 'ok'}
}

function progress(block, i) {
    var elem = block.find('.render .loading-slider');
    var percent = block.find('.render p');
    var width = 1;
    var id = setInterval(frame, i);
    function frame() {
        if (width >= 100) {
            clearInterval(id);
        } else {
            width++;
            elem.css('width', width + '%');
            percent.text(width + '%')
        }
    }
}

function uploadFileProgress() {
    var extblock = $(this).closest('.uploads-block-item');
    extblock.find('[data-events="upload_model"]').css('display', 'none');
    var block =  extblock.find('[data-target="upload_model"]');

    var fSExt = new Array('Bytes', 'KB', 'MB', 'GB'),
        i=0;while(fileSize>900){fileSize/=1024;i++;}
    var exactSize = (Math.round(fileSize*100)/100)+' '+fSExt[i];

    block.css('display','block');
    progress(block, time/100);
    setTimeout(function () {
        block.css('display','none');
        var finishBlock = extblock.find('[data-target="upload_model_finish"]');
        finishBlock.find('.p-size').text(exactSize);
        finishBlock.find('.render-a').text(fileName);
        finishBlock.css('display','block')
    }, time);
}



$(document).ready(function(){

    // 
    $('.del').live("click",function(){
        setRaiting($(this));
        return false;
    });

    $('.ok').live("click",function(){
        setRaiting($(this));
        return false;
    });

    $('.star').live('click',function(){
        var context=$(this);
        context.parent().find('.star').each(function(){
            $(this).removeClass('active');
        });
        context.addClass('active');
        $.get(context.attr('href'),function(data){
            var res= $.parseJSON(data);
            context.closest('.card_raiting').find('.number').html(res.rating+' / '+res.count);
            /*var cnt=1;
            context.parent().find('.star').each(function(){
                if(cnt==res.active){
                    $(this).addClass('active');
                }
                cnt++;
            });*/
        });
        return false;
    });

    $('.reiting_error').live('click',function(){
        reitingErrorMessageRemove();
    });

    function reitingErrorMessageRemove(){
        $('.reiting_error').each(function(idx,obj){
            $(this).remove();
        });
    }


    function setRaiting(context){
        reitingErrorMessageRemove();
        $.get(context.attr('href'),function(data){
            //var val=parseInt(data);
            var res= $.parseJSON(data);
            var val= parseInt(res.rating);
            var error_message= res.error_message;
            if(val>0){
                context.closest('.buttons').removeClass('null');
                context.closest('.reputation').removeClass('reputation_null');
                context.closest('.reputation-block').find('.reiting').html("+"+val);
                context.closest('.reputation-block').find('.reiting').removeClass('minus');
                context.closest('.reputation-block').find('.reiting').removeClass('null');
                context.closest('.reputation-block').find('.reiting').addClass('plus');
            }else if(val<0){
                context.closest('.buttons').removeClass('null');
                context.closest('.reputation').removeClass('reputation_null');
                context.closest('.reputation-block').find('.reiting').html(val);
                context.closest('.reputation-block').find('.reiting').removeClass('plus');
                context.closest('.reputation-block').find('.reiting').removeClass('null');
                context.closest('.reputation-block').find('.reiting').addClass('minus');
            }else{
                context.closest('.buttons').addClass('null');
                context.closest('.reputation').addClass('reputation_null');
                context.closest('.reputation-block').find('.reiting').html(val);
                context.closest('.reputation-block').find('.reiting').removeClass('plus');
                context.closest('.reputation-block').find('.reiting').removeClass('minus');
                context.closest('.reputation-block').find('.reiting').addClass('null');
            }

            if(error_message.length>0){
                //alert(error_message);
                context.parent().append('<div class="reiting_error">'+error_message+'</div>');
            }
        });
    }

    $('.ajax_form_submit').click(function(){

        if($(this).hasClass('disabled')){
            return false;
        }

        if($(this).hasClass('ajax_form_submit_confirm') || $(this).attr('message')){
            var r;
            if($(this).attr('message')){
                r=confirm($(this).attr('message'));
            }else{
                r=confirm("?");
            }
            if (r==false){
                return false;
            }
        }
        $(this).parent().find('.ajax_form').submit();
        return false;
    });

    $('.jq_confirm_link').click(function(){
        if($(this).attr('message')){
            r=confirm($(this).attr('message'));
        }

        if (r==false){
            return false;
        }
    });


    $('.ajax_link').click(function(){

        if($(this).hasClass('ajax_link_confirm')){
            r=confirm($(this).attr('message'));
        }

        if (r==false){
            return false;
        }

        var context=$(this);

        $.get($(this).attr('href'),function(data){
            if(data=='error'){
                alert('Error');
            }
            if(data=='ok'){
                eval(context.attr('on_success'));
            }
        });

        return false;

    });

    //     

    $('.delete_overlay').parent().hover(function(){
        $(this).find('.delete_overlay').show();
    },function(){
        $(this).find('.delete_overlay').hide();
    });

    $('.delete_overlay').click(function(e) {

        e.preventDefault();
        
        //Disabling gallery popup see this:685 
        // $(this).closest('.gallery_item').each(function (idx, elem) {
        //     $(elem).addClass('disable_popup');
        // });
        
        context = $(this);

        var confirmed=false;
        if($(this).attr('confirm')){
            var r=confirm($(this).attr('confirm'));
            if(r){
                confirmed=true;
            }
        }else{
            confirmed=true;
        }

        if(confirmed) {
            if ($(this).attr('wl') == 'true') {
                window.location = $(this).attr('url');
            } else {
                $.get($(this).attr('url'), function (data) {
                    if (data == 'ok') {
                        context.after('<div class="delete_fader"></div>');
                        context.remove();
                    }
                });
            }
        }
    });

    //       
