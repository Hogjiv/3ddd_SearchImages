    //Bookmark/Unbookmark ajax
    $('.bookmark_link').live('click',function(e){

        var url = $(this).attr('href');

        $.ajax({
            type: "GET",
            url: url,
            success: function(data){
                if($('.add_to_bookmarks').css('display')=='none') {
                    $('.remove_from_bookmarks').hide();
                    $('.add_to_bookmarks').show();
                }else{
                    $('.add_to_bookmarks').hide();
                    $('.remove_from_bookmarks').show();
                }                
            }
        });

        e.preventDefault();

    });
    
    $('.bookmark_toggle_link').click(function (e) {

        e.preventDefault();

        var url = $(this).attr('href');
        
        var context=$(this);

        $.ajax({
            type: "GET",
            url: url,
            success: function(data){

                var res=$.parseJSON(data);

                if(res && res.result=="added"){
                    if(context.hasClass('icon_heart')) {
                        context.removeClass('icon_heart');
                        context.addClass('icon_heart_red');
                    }else if(context.hasClass('icon_heart_small')){
                        context.removeClass('icon_heart_small');
                        context.addClass('icon_heart_red_small');
                    }
                }
                if(res && res.result=="removed"){
                    if(context.hasClass('icon_heart_red')) {
                        context.removeClass('icon_heart_red');
                        context.addClass('icon_heart');
                    }else if(context.hasClass('icon_heart_red_small')){
                        context.removeClass('icon_heart_red_small');
                        context.addClass('icon_heart_small');
                    }
                }
            }
        });
        
    })
    
    //Bookmark with collections
    $('.bookmark_form').submit(function (e) {

        e.preventDefault();

        var url = $(this).attr('action');

        var data=$(this).serialize();

        var context=$(this);
        
        $('.collections_placeholder').hide();

        $('.collections_placeholder_clock').show();

        $.ajax({
            type: "POST",
            url: url,
            data: data,
            success: function(data){

                var res=$.parseJSON(data);

                if(res && res.result=="added"){
                    var proto=context.data('prototype');
                    proto = proto.replace(/__collection.id__/g, res.collection_id);
                    proto = proto.replace(/__collection.title__/g, res.collection_title);
                    //context.find('.collections_placeholder').append(proto);
                    $('.collections_placeholder').each(function (idx, elem) {
                        if($(elem).closest('form').attr('entity_id')==context.attr('entity_id')){
                            $(elem).append(proto);
                        }else{
                            $(elem).append(proto.replace(/checked/g, ""));
                        }
                        
                    });
                    $('.new-cat').val('');                   
                    
                }else if(res && res.result=="removed"){
                    
                }else if(res && res.result=="error"){
                   
                }

                $('.collections_placeholder_clock').hide();

                $('.collections_placeholder').show();
            },
            error: function () {
                
                $('.collections_placeholder_clock').hide();

                $('.collections_placeholder').show();
                
            }
        });

    });


    $('.collection_checkbox').live('change',function (e) {

        e.preventDefault();

        var url = $(this).closest('.bookmark_form').attr('action');

        var data={
            'f[collection]': $(this).data('collectionid'),
        };

        var context=$(this);

        if (context.hasClass('inactive')) {
            return;
        }
        var collection_limit = 100;
        $.ajax({
            type: "POST",
            url: url,
            data: data,
            success: function (data) {

                var res = $.parseJSON(data);

                var collectionObjects = $(document).find('input[data-collectionid='+context.data('collectionid')+']')

                if (res && res.result == "added") {
                    context.attr('checked', 'checked');
                    $.ajax({
                        method: "GET",
                        url: "/bookmarks_collection/count/"+context.data('collectionid'),
                        success: function (data1) {
                            if (data1 && data1.success) {
                                var count = data1.data.count;
                                var shared = data1.data.shared;
                                if (shared===1 && count>=collection_limit) {
                                    var index;
                                    for (index = 0; index < collectionObjects.length; ++index) {
                                        var collectionObject = collectionObjects[index];
                                        if (!$(collectionObject).is(':checked')) {
                                            $(collectionObject).addClass('inactive');
                                            $(collectionObject).attr('disabled','disabled');
                                            $(collectionObject).parent().attr('title','      100 ');
                                        }
                                    }
                                }
                            }
                        }
                    });
                }
                if (res && res.result == "removed") {
                    context.removeAttr('checked');
                    $.ajax({
                        method: "GET",
                        url: "/bookmarks_collection/count/"+context.data('collectionid'),
                        success: function (data1) {
                            if (data1 && data1.success) {
                                var count = data1.data.count;
                                if (count<collection_limit) {
                                    var index;
                                    for (index = 0; index < collectionObjects.length; ++index) {
                                        var collectionObject = collectionObjects[index];
                                        $(collectionObject).removeClass('inactive');
                                        $(collectionObject).removeAttr('disabled');
                                        $(collectionObject).parent().removeAttr('title');
                                    }
                                }
                            }
                        }
                    });
                }
            }
        });
    })

