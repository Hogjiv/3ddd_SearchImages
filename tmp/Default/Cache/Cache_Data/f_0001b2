	 $('.scroll-pane').jScrollPane();
	}
	
$('.input_file input').live('change', function(){
			$(this).parents('.input_file').find('.val').text(this.value);
			$(this).blur();
								    })
$('.work-input_file input').live('change', function(){
		$(this).parents('.work-input_file').find('.val').text(this.value);
		$(this).blur();
	})
$('.select_block select').live('change', function(){
												
			var res = $(this).val();
			$(this.parentNode).find('.val').html($(this).find('option[value='+res+']').html())	;				 
								    })
$('.select_block select').live('keypress', function(){
								$(this).change();			  
												  })

$('.select_block select').each(function(){
                    $(this).change();           
                                        })

