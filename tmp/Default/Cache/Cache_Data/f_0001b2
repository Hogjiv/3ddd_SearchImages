$('#messages_block .sales_statistics_table input.checkbox').live('click', function(){
	var cc = this;
	var dd = 0;
	$(cc).parents('.sales_statistics_table').find('input.checkbox').each(function(){
			if ($(this).attr('checked')=='checked' || $(this).attr('checked')==true){
				 dd = 1;
			}
			})
			if (dd==1){
				$(cc).parents('.message_section').find('.button_grey').removeClass('unactive').addClass('active').removeAttr('disabled');
			}
			else {
				$(cc).parents('.message_section').find('.button_grey').removeClass('active').addClass('unactive').attr('disabled',true);
			}
																					 })

$('.delete_models_from_zakl .checkbox').live('click', function(){
															  
	var cc = this;
	var dd = 0;
	$(cc).parents('form').find('input.checkbox').each(function(){
			if ($(this).attr('checked')=='checked' || $(this).attr('checked')==true){
				 dd = 1;
			}
			})
			if (dd==1){
				$(cc).parents('form').find('.button_grey').removeClass('unactive').addClass('active').removeAttr('disabled');
			}
			else {
				$(cc).parents('form').find('.button_grey').removeClass('active').addClass('unactive').attr('disabled',true);
			}
																					 })


$('#messages_block .sales_statistics_table th input.checkbox.all').live('click', function(){
			var cc1 = this;
			var fgh = 1;
			if ($(cc1).attr('checked')=='checked' || $(this).attr('checked')==true){
				fgh = 1;
				$(cc1).parents('.message_section').find('.button_grey').removeClass('unactive').addClass('active').removeAttr('disabled');
			}
			else {
				fgh = 0;
				$(cc1).parents('.message_section').find('.button_grey').removeClass('active').addClass('unactive').attr('disabled',true);
			}
			$(cc1).parents('.sales_statistics_table').find('input.checkbox').each(function(){
					if (fgh==1){
					$(this).attr('checked', 'checked');
					$(this).attr('checked', true);
					}
					else {
					$(this).removeAttr('checked');
					}
																							    })
																				   })

		
$('#unfolded_chat_block .form_block .textarea textarea, #coments_block .form textarea').live('focus', function(){
									$(this).parents('label').find('.placeholder').hide();									
																		})
$('#unfolded_chat_block .form_block .textarea textarea, #coments_block .form textarea').live('blur', function(){
																			   if($(this).val()==''){
									$(this).parents('label').find('.placeholder').show();			
																			   }
																		})
						   
