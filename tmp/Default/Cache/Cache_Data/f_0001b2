    $('[data-events="check-color"]').change(function () {
        var id = $(this).attr('id');
        if($(this).is(':checked')){
            $("label[for="+ id +"]").addClass('checked');

            var countCahcked = ($('.select-color input[type=checkbox]:checked')).length;
            if(countCahcked > 3){
                var first = $('.select-color input[type=checkbox]:checked').first();

                var pos_checked = $(this).data('pos');
                var pos_first = first.data('pos');

                if(pos_checked > pos_first){
                    first.removeAttr("checked");
                    $("label[for="+ first.attr('id') +"]").removeClass('checked');
                }else {
                    var last = $('.select-color input[type=checkbox]:checked').last();
                    var pos_last = last.data('pos');
                    last.removeAttr("checked");
                    $("label[for="+ last.attr('id') +"]").removeClass('checked');
                }
            }
        }else {
            $("label[for="+ id +"]").removeClass('checked');
        }
    });


